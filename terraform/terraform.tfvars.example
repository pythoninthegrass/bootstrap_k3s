# libvirt_uri         = "qemu:///system"
libvirt_uri         = "qemu+ssh://<USERNAME>:<PASSWORD>@<IP_ADDRESS>:22/system?sshauth=ssh-password"
control_plane_count = 3
worker_count        = 1
memory_control      = 2048
memory_worker       = 4096
cpu_control         = 2
cpu_worker          = 2
disk_size           = 32
base_ip             = "192.168.56"
network_name        = "k8s_network"
use_bridge_network  = false
ssh_public_key      = "ssh-rsa AAAAB..."
github_user         = "GITHUB_USERNAME"
